    .timeline {
        margin: 0px auto 0px;
        position: relative;
        transition: all 0.4s ease 0s;
        width: 100%;
        min-height: 470px;
        &:before {
            background: #e1e1e1;
            content: "";
            height: 100%;
            left: 20px;
            position: absolute;
            top: 0;
            width: 3px;
            padding-bottom: 999px;
        }
        &:after {
            clear: both;
            content: "";
            display: table;
            width: 100%;
        }
        &:first-child {
            margin: 40px 0px 0px 0px;
        }
    }
    .timeline-item {
        float: left;
        width: 100%;
        position: relative;
        margin: 25px 0px 0px 0px;
    }
    .timeline-icon {
        border: 2px solid #fff;
        background: #36415d;
        border-radius: 12px;
        height: 12px;
        width: 12px;
        left: 18px;
        margin-left: -2px;
        overflow: hidden;
        position: absolute;
        top: 0;
    }
    .timeline .col-content:before {
        content: url('../images/right-tip.png');
        position: absolute;
        left: 31px;
        top: -2px;
    }
    .timeline .col-content {
        margin-left: 40px;
        margin-top: -10px;
        width: 285px;
        border: 1px solid #e4e9ec;
        padding: 10px 15px;
        background: #f6f8fa;
