/*------- World Clock ----------- */
.world-clock{
    .panel-body{
        background: #363636 ;
        .my-space-clock{
            width: 140px;
            height: 140px;
            margin: 0px auto;
            position: relative;
            .scale(0.8);
           .dial-board{
                position: absolute;
                width: 100%;
                height: 100%;
                .border-radius(50%);
                background: #292929;
                border: 5px solid #292929;
                > div{
                    width: 1px;
                    height: 100%;
                    border: solid @color-2;
                    border-width: 5px 0;
                    position: absolute;
                    left: 50%;
                    opacity: 0.9;
                    margin-left: -1px;
                    &:nth-child(1){
                        .rotate(30deg);
                    }
                    &:nth-child(2){
                        .rotate(60deg);
                    }
                    &:nth-child(3){
                        .rotate(90deg);
                    }
                    &:nth-child(4){
                        .rotate(120deg);
                    }
                    &:nth-child(5){
                        .rotate(150deg);
                    }
                }
                
            }
            #clock{
                position: absolute;
                width: 100%;
                height: 100%;
                background: none;
                .border-radius(50%);
                padding: 0;
                margin: 0;
                #sec, #min, #hour{
                    width: 5px;
                    height: 100%;
                    background:url(../images/needle_seconds.png)no-repeat ;
                    background-size: 100%;
                    
                    left: 50%;
                    margin: -2px 0 0 -1px;
                    
                }
                #min{
                    background:url(../images/needle_minute.png)no-repeat ; 
                    background-size: 100%;
                   
                }
                #hour{
                    background:url(../images/needle_hour.png)no-repeat  ;
                    background-size: 100%;
                   
                }
            }
        
        }
        .dropdown{
            color:@color-2;
            max-width: 200px;
            margin:0 auto;
            
            p{
                display: inline-block;
                margin: 0;
                .font-bold;
                color: @color-2;
                line-height: 1.3;
                text-transform:capitalize;
                span{
                    .font-light;
                    display: block;
                }
            }
            .dropdown-toggle{
                .glyphicon{
                   color:@color-2;
                    padding: 1px;
                    vertical-align: top;
                }
            }
            .dropdown-menu{
                width: 100%;
               /* max-height: 200px;
                overflow: auto;*/
                li p{
                   /* padding: 5px 10px;*/
                    color: @color-1;
                    opacity: 0.6;
                    overflow: hidden;
                    margin: 0;
                    text-overflow: ellipsis;
                    display: block;
                    span{
                        display: inline-block;
                    }
                    &:hover{
                        cursor: pointer;
                        background: #efefef;
                    }
                }
            }
        }
    }
}

/* ----------Calendar Card --------- */
.tile-group.calendar{
    .panel{
        .panel-body{
            padding:0;
        }
    }
}
.my-space-calendar{
    width: 100%;
    height: 100%;    
    .k-calendar{
        width: 100%;
        border:none;
        .k-header{
            display: none;
        }
        table{
            border:none;
            thead{
                th{
                    border-color: transparent;
                    opacity: 0.6;
                    text-align: center;
                    font-size: 0.8em;
                    padding: 12px 0 0;
                }
            }
            tbody{
                td{
                    text-align: center;
                    &:hover, .k-state-selected, .k-state-focused, .k-state-selected.k-state-focused{
                        background: none;
                        box-shadow: none;
                        
                    }
                    .k-link{
                        color: @color-1;
                    }
                    &.k-today{
                        .k-link{
                            background:  @color-4;
                            color: @color-2;
                            &:before{
                                position: absolute;
                                width: 4px;
                                height: 4px;
                                background: @color-2;
                                left: 50%;
                                bottom: 2px;
                                margin-left: -2px;
                                content: "";
                                .border-radius(50%);
                            }
                        }
                    }
                    &.k-other-month{
                        opacity: 0.3;
                    }
                    .k-link{
                        padding: 0;
                        height: 28px;
                        width: 28px;
                        line-height: 28px;
                        margin: 0 auto;
                        border-radius: 50%;
                        position: relative;
                        
                    }
                    
                }
            }
        }
    }
}

/* --------- Weather ---------- */
 .tile-group.weather{
    .panel{
       .panel-body {
         position: relative;
          background-size: cover; 
           .banner-overlay{ 
                background: rgba(0,0,0, 0.16); 
            }
        }
    }
    .dropdown{
        display: inline-block;
        
         
        .dropdown-toggle{
            padding: 0;
            position: relative;
            padding-right: 30px;
            cursor: pointer;
            p{
                color: @color-2;
                text-transform: capitalize;
                font-size: @base-font + 14;
                .font-light;
                span{
                    display: block;
                    font-size: 0.7em;
                    padding-top: 8px;
                }
            }
            i.glyphicon{
                position: absolute;
                right: 0;
                top:11px;
                color: @color-2;
            }
            
             
        }
        .dropdown-menu{
            /*max-height: 200px;*/
            padding: 10px 0;
            &:before{
                left:50%;
                margin-left: -8px;
            }
            li{
                padding: 2px 30px 2px 10px;
                p{
                    white-space: nowrap;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    line-height: 1.68;
                    span{
                        font-size: 0.8em;
                    }
                }
                &:hover{
                    cursor: pointer;
                    background: #efefef;
                }
            }
        }
         
    }
     .temperature{
        padding: 15px 0 0;
        transition: all 0.3s;
         position: relative;
         z-index: 2;
         img{
            margin-right: 15px;
         }
         h2{
            display: inline-block;
            color: @color-2;
            vertical-align: middle;
            .font-light;
             font-size: @base-font + 38;
             height: auto;
             sup{
                font-size: 50%;
                 padding: 0 5px;
                 top:-0.9em;
             }
         }
         &.zoom-out{
            .scale(0.3);
             opacity: 0.3;
                
         }
         h5 {
             color: @color-2;
         }
     }
}

