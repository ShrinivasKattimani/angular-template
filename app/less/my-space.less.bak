        }
        
    }
    .panel-body{
        height: auto;
        .k-chart{
            margin: 0 auto;
        }
    }
    .panel-footer {
        background: none;
        border: solid @color-9;
        border-width: 0 1px 1px 1px;
        .text-left;
        p {
            display: inline-block;
            padding: 10px;
            .font-bold;
            opacity: 0.6;
            margin: 0;
        }
    }
    .table-view {
        margin: 0;
        table {
            table-layout: auto;
        }
        tr {
            td {
                padding: 16px 10px;
                font-size: @base-font - 2;
                .text-right;
                &.title {
                    .text-left;
                }
                p {}
                p + span {
                    font-size: 0.8em;
                    opacity: 0.5;
                    padding-top: 8px;
                    display: block;
                }
                /* p + span */
            }
            /* td */
            &.total {
                td {
                    p {
                        color: @color-4;
                    }
                    &.title p {
                        color: @color-1;
                        text-transform: uppercase;
                    }
                }
            }
            /* total */
        }
        /* tr */
        thead {
            tr {
                td {
                    font-size: 0.8em;
                }
            }
        }
        /* th */
    } /* Table View */
    .matrix-graph{
        height: 150px;
        img{ max-width: 100%;}
        
    }  /* Matrix graph */
    .legends{
        text-align: left;
        padding: 20px 0 0;
        p{
            display: inline-block;
            position: relative;
            padding: 0 4px 0 0;
            text-transform: uppercase;
            font-size: @base-font - 6;
            @media (min-width:1200px){
                 padding: 0 10px 0 0;
                font-size: @base-font - 5;
                &:before{
                    width: 30px;
                }
            }
            &:before{
                content: "";
                width: 20px;
                height: 3px;
                .border-radius(2px);
                position: absolute;
                top:-10px;
                left: 0;
                background: #ccc;
            }
            &.basic{
                &:before{
                    background: @color-16;
                }
            }
            &.housing{
                &:before{
                    background: @color-15;
                }
            }
            &.education{
                &:before{
                    background: @color-17;
                }
            }
            &.food{
                &:before{
                    background: @color-19;
                }
            }
            &.transport{
                &:before{
                    background: @color-18;
                }
            }
            
        }
    }
}

